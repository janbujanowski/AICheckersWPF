<Application x:Class="AI_Checkers.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AI_Checkers"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundBrush" Color="#ece4d8" />
        <SolidColorBrush x:Key="HeaderBrush" Color="#7c334f" />
        <SolidColorBrush x:Key="HighLightBrush" Color="#544943" />
        <SolidColorBrush x:Key="WhiteField" Color="#544943" />
        <SolidColorBrush x:Key="BlackField" Color="Black" />
        <local:EnumToStringConverter x:Key="EnumToStringConverter"/>
    </Application.Resources>
</Application>
